spring: 
  application: 
    name: eurekaserver
  cloud: 
    config: 
      uri: ${CONFIG_SERVER.URI:http://localhost:8888/}
debug: true
logging: 
  pattern: 
    console: ${spring.application.name}-%clr(%d{yyyy-MM-dd HH:mm:ss.SSS}){faint} %clr(%5p) %clr(${PID:- }){magenta} %clr(---){faint} %clr([%15.15t]){faint} %clr(%-40.40logger{39}){cyan} %clr(:){faint} %m%n%wEx