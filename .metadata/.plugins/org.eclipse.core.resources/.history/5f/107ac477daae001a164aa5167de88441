package com.swarm.base.entity;

import javax.persistence.Column;
import javax.persistence.Entity;

import lombok.Data;

@Data
@Entity
public class SysRole extends BaseEntity {
	
	/**
	 * 角色名称
	 */
	@Column(length = 20)
	private String name;
	
	/**
	 * 角色描述
	 */
	@Column(length = 100)
	private String desc;
	
}
